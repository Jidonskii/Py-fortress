number = 10
second_number = 10
first_array = ["John"]
second_array = [1,2,3]

if number < 15:
    print("1")

if "John" in first_array :
    print("2")

if len(second_array) == 3:
    print("3")

if len(first_array) + len(second_array) == 4:
    print("4")

if first_array and first_array[0] == "John":
    print("5")

if second_number != 6:
    print("6")
